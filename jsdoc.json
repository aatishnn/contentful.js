{
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": [
        "node_modules/better-docs/typescript"
    ],
    "source": {
        "includePattern": "\\.(js|ts)$",
        "include": [
            "lib/",
            "./README.md"
        ]
    },
    "opts": {
        "encoding": "utf8",
        "template": "node_modules/better-docs",
        "destination": "out/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        }
    },
    "docdash": {
        "static": false,
        "sort": true
    }
}
